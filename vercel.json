{
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/uploads/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/uploads/(.*)",
        "dest": "/static/uploads/$1"
      },
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  
  